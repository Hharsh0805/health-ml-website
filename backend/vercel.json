{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "rewrites": [
      {
        "source": "/api/gemini",
        "destination": "/server.js"
      },
      {
        "source": "/(.*)",
        "destination": "/server.js"
      }
    ],
    "env": {
      "GEMINI_API_KEY": "@GEMINI_API_KEY"
    },
    "cleanUrls": true
  }
  